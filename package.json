{
	"name": "dock-rush",
	"version": "1.0.2",
	"description": "A modern documentation system for React applications with versioning, multi-language support, and markdown-based content",
	"keywords": [
		"documentation",
		"react",
		"markdown",
		"docs",
		"vite",
		"plugin",
		"versioning",
		"i18n"
	],
	"main": "./dist/client.cjs",
	"module": "./dist/client.mjs",
	"types": "./dist/client.d.ts",
	"exports": {
		".": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.cjs",
			"default": "./dist/client.mjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.cjs",
			"default": "./dist/client.mjs"
		},
		"./server": {
			"types": "./dist/plugin.d.ts",
			"import": "./dist/plugin.mjs",
			"require": "./dist/plugin.cjs",
			"default": "./dist/plugin.mjs"
		},
		"./plugin": {
			"types": "./dist/plugin.d.ts",
			"import": "./dist/plugin.mjs",
			"require": "./dist/plugin.cjs",
			"default": "./dist/plugin.mjs"
		},
		"./style.css": "./dist/style.css"
	},
	"author": {
		"name": "da-b1rmuda",
		"url": "https://github.com/da-b1rmuda"
	},
	"license": "MIT",
	"homepage": "https://github.com/da-b1rmuda/dock-rush#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/da-b1rmuda/dock-rush.git"
	},
	"bugs": {
		"url": "https://github.com/da-b1rmuda/dock-rush/issues"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "pnpm run build:ts && pnpm run build:css",
		"build:ts": "tsup --no-watch",
		"build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/style.css --minify"
	},
	"peerDependencies": {
		"react": "*",
		"react-dom": "*"
	},
	"devDependencies": {
		"@radix-ui/react-slot": "^1.2.4",
		"@tailwindcss/cli": "^4.1.17",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"tsup": "^8.5.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@types/react-syntax-highlighter": "^15.5.13",
		"fast-glob": "^3.3.3",
		"gray-matter": "^4.0.3",
		"lucide-react": "^0.561.0",
		"react-markdown": "^10.1.0",
		"react-syntax-highlighter": "^16.1.0",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.1"
	}
}
